/*
 * 2) Ler um número N e depois uma matriz quadrada NxN com números inteiros.
 * Depois, mostrar na tela a soma dos
elementos de cada linha da matriz.
Entrada:
A entrada contém o número N, depois os dados da matriz.
Saída:
A saída contém os números representando a soma dos elementos de cada linha da matriz.

 */

package matrizes;

import java.util.Scanner;

public class Mt02 {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int N = sc.nextInt();
		
		int[][] valores = new int[N][N];
		
		for (int i=0; i<N; i++) {
			for (int j=0; j<N; j++) {
				valores[i][j] = sc.nextInt();
			}
		}
		
		for (int i=0; i<N; i++) {
			int soma = 0;
			for (int j=0; j<N; j++) {
				soma += valores[i][j];
			}
			System.out.println(soma);
		}
		
		sc.close();
	}
}