package e_matrizes;

import java.util.Locale;
import java.util.Scanner;

public class Mt06 {
	
	public static void main(String[] args) {
		
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		int N = sc.nextInt();
		
		int[][] valores = new int[N][N];
		
		int somaP = 0;
		
		for (int i=0; i<N; i++) {
			for (int j=0; i<N; j++) {
				valores[i][j] = sc.nextInt();
				if (valores[i][j] > 0) {
					somaP += valores[i][j];
				}
			}
		}
		
		System.out.println();
		System.out.println();
		System.out.println();
		System.out.println();
		System.out.println("MATRIZ ALTERADA:");
		System.out.println();
		
		sc.close();
	}
}