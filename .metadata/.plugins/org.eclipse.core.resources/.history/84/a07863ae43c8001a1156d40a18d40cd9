package estrutura_sequencial;

import java.util.Scanner;

public class Uri1003 {

	public statfic void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int A = sc.nextInt();
		int B = sc.nextInt();
		
		int SOMA = A + B;
		
		System.out.println("SOMA = " + SOMA);
		
		sc.close();
	}
}