package matrizes;

import java.util.Scanner;

public class Mt02 {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int N = sc.nextInt();
		
		int[][] valores = new int[N][N];
		
		for (int i=0; i<N; i++) {
			int soma = 0;
			for (int j=0; j<N; j++) {
				valores[i][j] = sc.nextInt();
				soma += valores[i][j];
			}
			System.out.println(soma);
		}
		
		for (int i=0; i<N; i++) {
			int soma = 0;
			for (int j=0; j<N; j++) {
				valores[i][j] = sc.nextInt();
				soma += valores[i][j];
			}
			System.out.println(soma);
		}
		
		sc.close();
	}
}