package e_matrizes;

import java.util.Scanner;

public class Mt07 {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int M = sc.nextInt();
		int N = sc.nextInt();
		
		double[][] valores = new double[M][N];
		
		for (int i=0; i<N; i++) {
			for (int j=0; j<N; j++) {
				valores[i][j] = sc.nextDouble();
			}
		}
		
		int linha = sc.nextInt();
		
		/*double ultimaP = valores[linha][N-1];
		System.out.println(ultimaP);*/
		
		sc.close();
	}
}